{
	"name": "flowlet1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          cnpj as string,",
				"          valor as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 derive(valor = toDouble(valor)) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(cnpj),",
				"     soma_valor = sum(valor)) ~> aggregate1",
				"aggregate1 output() ~> output1"
			]
		}
	}
}