{
	"name": "flowlet1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          cnpj as string,",
				"          razao_social as string,",
				"          endereco as string,",
				"          cidade as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 derive(cnpj_num = replace(replace(replace(cnpj, '/', ''), '.',''), '-','')) ~> derivedColumn1",
				"derivedColumn1 output() ~> output1"
			]
		}
	}
}